{
  "version": 2,
  "builds": [
    {
      "src": "src/api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "src/api/.*",
      "dest": "src/api/server.js"
    },
    {
      "src": "src/(.*)",
      "dest": "src/client.js"
    }
  ]
}